            <div class="form-container fee-details hidden">
                <form class="fee-details-form" action="/submit-form" method="post" enctype="multipart/form-data">
                    <div role="main" class="form-all">
                        <div class="form-header">
                            <h1 data-component="header">Registration Form</h1>
                            <h6>Fill out the form carefully for registration</h6>
                        </div>
        
                        <h2 class="subform-header" data-component="header">Fee Details:</h2>
        
                            <h4>Institute Fee:</h4>
                            <div class="form-details">
        
                                <div class="input-control">
