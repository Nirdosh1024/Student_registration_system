            <!--Pay Now form-->
        
            <div class="form-container payment hidden">
                <form class="payment-form" action="/submit-form" method="post">
                    <div role="main" class="form-all">
                        <div class="form-header">
                            <h1 data-component="header">Registration Form</h1>
                            <h6 >Fill out the form carefully for registration</h6>
                        </div>
        
